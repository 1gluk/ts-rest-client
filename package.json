{
  "name": "ts-rest-client",
  "version": "1.0.0",
  "description": "Simple way to create REST API clients",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "files": [
    "lib/**/*"
  ],
  "scripts": {
    "test": "jest --config jestconfig.json",
    "test:watch": "npm run test -- --watch",
    "coverage": "npm run test -- --coverage",
    "build": "tsc",
    "lint": "tslint --project tsconfig.json",
    "prepare": "npm run build",
    "prepublishOnly": "npm test && npm run lint",
    "preversion": "npm run lint",
    "version": "git add -A src",
    "postversion": "git push && git push --tags"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lukaskopenec/ts-rest-client.git"
  },
  "keywords": [
    "Typescript",
    "REST"
  ],
  "author": "Lukas kopenec",
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/lukaskopenec/ts-rest-client/issues"
  },
  "homepage": "https://github.com/lukaskopenec/ts-rest-client#readme",
  "devDependencies": {
    "@types/jest": "^24.0.6",
    "jest": "^24.1.0",
    "rxjs": "^6.4.0",
    "ts-jest": "^24.0.0",
    "tslint": "^5.12.1",
    "typescript": "^3.3.3"
  },
  "peerDependencies": {
    "rxjs": "~6.3.0"
  }
}
